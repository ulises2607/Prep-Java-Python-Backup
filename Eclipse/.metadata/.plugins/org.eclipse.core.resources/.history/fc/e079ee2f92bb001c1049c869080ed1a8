import java.util.*;
public class Actividad2 {

	public static void main(String[] args) {
		//LLmar a nuestr clase
		Scanner sc = new Scanner(System.in);
		System.out.println("Ingrese 2 series");
		for (int i=0;i<2;i++) {
		String n = sc.next();
		int t = sc.nextInt();
		String d = sc.next();
		String c = sc.next();
		
				
		serie nuevaSerie = new serie(n, t, d, c);
		
		nuevaSerie.mostrar();
		}
	}

}
