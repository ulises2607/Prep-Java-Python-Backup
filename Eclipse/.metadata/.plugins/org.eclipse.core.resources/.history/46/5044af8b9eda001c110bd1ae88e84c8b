public class EscribeArray {

	public static void main(String[] args) throws IOException {
		
        String s = "Hola tengo que aprender esto";
        
        byte array_1[] = s.getBytes();
        
       
        
       for(int i=0;i<array_1.length;i++) {
    	   
    	   System.out.println(array_1);
       }
        }
	 
	}

