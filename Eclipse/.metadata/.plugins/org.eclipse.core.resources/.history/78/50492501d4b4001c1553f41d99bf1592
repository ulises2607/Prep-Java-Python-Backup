import java.util.Scanner;
public class ARRAY2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		//Se ingresara el tama単o del vector
		System.out.println("Ingrese tama単o del vector");
		int tama単ov = sc.nextInt();
		//INTENTAR ARMAR EL VETOR
		for (int i=0;i<tama単ov;i++) {
			System.out.println("Ingrese elemento "+(i+1));
			int elemento = sc.nextInt();
			int vector []= {elemento};
			
			for(int vec:vector) {
				System.out.println(vec);
			}
			
		}
		

	}

}
